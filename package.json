  "dependencies": {
    "@types/node": "^20.3.1",
    "downloadjs": "^1.4.7",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "vitepress": "^1.0.0-rc.31",
    "vue": "^3.2.47",
    "vue-doc-exporter": "^1.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/downloadjs": "^1.4.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
