{
  "name": "vue-doc-exporter",
  "author": "Kelvin Chidothi",
  "version": "1.2.9",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KellsWorks/vue-doc-download.git"
  },
  "main": "./dist/vue-doc-download.umd.cjs",
